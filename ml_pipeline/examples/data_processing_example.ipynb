   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 为每个类别选择一个原始窗口和一个增强窗口\n",
    "plt.figure(figsize=(15, 12))\n",
    "\n",
    "for i, label in enumerate(unique):\n",
    "    # 获取该类别的原始窗口\n",
    "    orig_class_windows = normalized_windows[labels == label]\n",
    "    \n",
    "    # 获取该类别的增强窗口\n",
    "    aug_class_windows = augmented_windows[augmented_labels == label]\n",
    "    aug_class_windows = aug_class_windows[len(orig_class_windows):]  # 只选择新增的窗口\n",
    "    \n",
    "    if len(aug_class_windows) == 0:\n",
    "        continue\n",
    "    \n",
    "    # 选择窗口\n",
    "    orig_window = orig_class_windows[0]\n",
    "    aug_window = aug_class_windows[0]\n",
    "    \n",
    "    # 绘制加速度数据\n",
    "    plt.subplot(3, 2, i*2+1)\n",
    "    plt.plot(orig_window[:, 0], 'b-', label='原始 X')\n",
    "    plt.plot(aug_window[:, 0], 'r--', label='增强 X')\n",
    "    plt.title(f'类别 {int(label)} ({[\"静止\", \"走路\", \"跑步\"][int(label)]}) - X轴加速度比较')\n",
    "    plt.xlabel('时间点')\n",
    "    plt.ylabel('标准化加速度')\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "    \n",
    "    # 绘制陀螺仪数据\n",
    "    plt.subplot(3, 2, i*2+2)\n",
    "    plt.plot(orig_window[:, 3], 'b-', label='原始 X')\n",
    "    plt.plot(aug_window[:, 3], 'r--', label='增强 X')\n",
    "    plt.title(f'类别 {int(label)} ({[\"静止\", \"走路\", \"跑步\"][int(label)]}) - X轴陀螺仪比较')\n",
    "    plt.xlabel('时间点')\n",
    "    plt.ylabel('标准化角速度')\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
