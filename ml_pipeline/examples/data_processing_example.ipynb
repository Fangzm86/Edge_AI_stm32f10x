   "source": [
    "## 5. 创建窗口\n",
    "\n",
    "将时间序列数据分割成固定长度的窗口。"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 设置窗口参数\n",
    "window_length = 128  # 窗口长度\n",
    "overlap = 0.5  # 重叠率\n",
    "feature_columns = ['accel_x', 'accel_y', 'accel_z', 'gyro_x', 'gyro_y', 'gyro_z']\n",
    "label_column = 'label'\n",
    "\n",
    "# 创建窗口\n",
    "windows, labels, scaler = create_windows(\n",
    "    df,\n",
    "    window_length=window_length,\n",
    "    overlap=overlap,\n",
    "    feature_columns=feature_columns,\n",
    "    label_column=label_column\n",
    ")\n",
    "\n",
    "print(f\"创建了 {len(windows)} 个窗口\")\n",
    "print(f\"窗口形状: {windows.shape}\")\n",
    "print(f\"标签形状: {labels.shape}\")\n",
    "\n",
    "# 查看标签分布\n",
    "unique, counts = np.unique(labels, return_counts=True)\n",
    "print(\"\\n标签分布:\")\n",
    "for u, c in zip(unique, counts):\n",
    "    print(f\"  类别 {int(u)} ({['静止', '走路', '跑步'][int(u)]}): {c} 个窗口 ({c/len(labels)*100:.1f}%)\")"
   ]
  },
  {
