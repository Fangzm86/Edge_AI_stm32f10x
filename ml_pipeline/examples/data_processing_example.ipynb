    "plt.figure(figsize=(15, 12))\n",
    "\n",
    "for i, label in enumerate(unique):\n",
    "    # 获取该类别的窗口\n",
    "    class_windows = normalized_windows[labels == label]\n",
    "    \n",
    "    # 选择第一个窗口\n",
    "    window = class_windows[0]\n",
    "    \n",
    "    # 绘制加速度数据\n",
    "    plt.subplot(3, 2, i*2+1)\n",
    "    plt.plot(window[:, 0], label='X')\n",
    "    plt.plot(window[:, 1], label='Y')\n",
    "    plt.plot(window[:, 2], label='Z')\n",
    "    plt.title(f'标准化后 - 类别 {int(label)} ({[\"静止\", \"走路\", \"跑步\"][int(label)]}) - 加速度')\n",
    "    plt.xlabel('时间点')\n",
    "    plt.ylabel('标准化加速度')\n",
    "    plt.legend()\n",
